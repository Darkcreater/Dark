<script>
    function showMaterialInfo(materialNumber) {
        // Hide all material info sections
        var materialInfos = document.querySelectorAll(".material-info");
        materialInfos.forEach(function(info) {
            info.style.display = "none";
        });

        // Show the selected material info section
        var materialInfo = document.getElementById("material" + materialNumber + "Info");
        materialInfo.style.display = "block";
    }

    function showSolution(solutionName) {
        // Hide all solution info sections
        var solutionInfos = document.querySelectorAll(".solution-info");
        solutionInfos.forEach(function(info) {
            info.style.display = "none";
        });

        // Show the selected solution info section
        var solutionInfo = document.getElementById(solutionName + "Info");
        solutionInfo.style.display = "block";
    }
</script>
